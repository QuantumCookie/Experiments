package utilities;

class GTimerTest implements GListener
{
    private GTimer timer;
    
    public GTimerTest()
    {
        timer = new GTimer(this);    
    }
    
    public void start()
    {
        timer.start();
    }
    
    public static void main(String[] args)
    {
        GTimerTest test = new GTimerTest();
        test.start();
    }
}